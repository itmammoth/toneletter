  },
  "devDependencies": {
    "cryptiles": ">=4.1.2",
    "hoek": "^6.1.3",
    "http-server": "^0.11.2",
    "jasmine-core": "^2.99.1",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "^2.2.0",
